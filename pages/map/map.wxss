/* pages/map/map.wxss */
page {
  height: 100%;
}
.tab{
  height: 120rpx;
  width: 100%;
  white-space: nowrap;
  box-sizing: border-box;
}
.icon{
  width: 48rpx;
  height:48rpx;
}

.tab view{
  width:20%;
  text-align: center;
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.page-section-gap{
  width: 100%;
  box-sizing: border-box;
  height: 100%;
}
#myMap{
  width: 100%;
  height: 100%;
}

