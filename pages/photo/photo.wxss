/* pages/photo/photo.wxss */

page {
  height: 100%;
}

.swiper {
  width: 100%;
  display: block;
  height: 100%;
  margin-top: 30rpx;
  overflow: hidden;
  margin-bottom: 10px;
}

.swiper swiper-items {
  height: 100%;
}

.photo-list {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding-left: 30rpx;
  flex-flow: wrap;
}
.photoBox{
  display: inline-block;
  width: 45%;
  margin-right: 10rpx;
  margin-top: 10rpx;
  height: 346rpx;
  overflow: hidden;
}



